<Window x:Class="Snake.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Snake"
        mc:Ignorable="d"
        Title="Snake" Height="700" Width="700"
        KeyDown="IsKeyDown">
    <Canvas x:Name="PlayField">
        <Image x:Name="field" Width="500" Height="500"></Image>
        <Label x:Name="lbStart" Canvas.Left="249" Canvas.Top="306" FontSize="25">Press Space to Start</Label>
        <Label x:Name="lbScore" Canvas.Left="604" Canvas.Top="28">Score:</Label>
    </Canvas>

</Window>
